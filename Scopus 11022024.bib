	keywords = {Agricultural robots; Antennas; Drones; Energy transfer; Flow graphs; Wakes; Wireless power transfer; Distributed heuristics; Ground infrastructures; Internet of thing (IoTs); Large-scale applications; Multi hop communication; Path planning problems; Synchronization problem; Wireless sensor network (WSNs); Wireless sensor networks},
	issn = {13891286},
