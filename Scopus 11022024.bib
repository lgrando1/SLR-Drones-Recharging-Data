	keywords = {Cognitive systems; Disasters; Fuzzy logic; Fuzzy rules; Industrial robots; MATLAB; Multipurpose robots; Swarm intelligence; Application fields; Disaster rescue; Fuzzy cognitive map; Fuzzy logic controllers; Multi-robot exploration; Multirobot systems; Rescue operations; Stopping criteria; Ant colony optimization},
