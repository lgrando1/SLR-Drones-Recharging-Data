@BOOK{Nidhi2023239,
	author = {Nidhi and Kumar, Jitender and Sandhu, Sofia},
	title = {Moving Toward Autonomous Vehicles (Drones and Robots) for Efficient and Smart Delivery of Services Using Hybrid Ontological-Based Approach},
	year = {2023},
	journal = {Convergence of IoT, Blockchain, and Computational Intelligence in Smart Cities},
	pages = {239 – 250},
	doi = {10.1201/9781003353034-13},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85179201008&doi=10.1201%2f9781003353034-13&partnerID=40&md5=c7149ee00642ee2e1a6632f5b21b75e3},
	abstract = {Everything is getting smarter nowadays, from mobiles to lifestyle, then why cannot be smart delivery services? Increased travel demand in urban areas is one of the salient factors of congestion on the existing network. With this tremendous rate of increase, existing infrastructure is not competent to meet the rising demand, nor is it sustainable point to build multiple roads at every place and ongoing routes. It is not feasible every time to increase resources which in turn will lead to more delays and congestion. So, there is an utmost need to explore the emerging autonomous technologies that can minimize the process of human-to-human interaction and ensure reliable, fastest, and efficient delivery of transport packages, food items, or other postal delivery services. It’s time to integrate small unmanned aerial vehicles (UAVs) into intelligent transportation systems that will empower efficient and smart delivery of services. Consider a case of food delivery process which is being transformed digitally by the use of smartphones and tracking apps. Nevertheless, the digital ordering of food via apps has made the process easier and relaxing but it has failed to mitigate congestion problem in urban cities. Now, there is a need to move one more step higher by making transportation services smart, reliable, and congestion free. The solution to this smart transportation services lies in the employment of Autonomous Ground Vehicles (AGVs) and Autonomous Aerial Vehicles (AAVs). The former vehicle system (i.e. AGV) applies to generic term that signifies autonomous cars or autonomous robots with varying sizes and weights traveling specific distances that the parcel needs to be delivered to, whereas the latter vehicle system (i.e. AAV) represents drones or unmanned vehicles that are being utilized to transport packages, providing healthcare services, postal and food delivery services, as well as other utility services in the case of emergency situations. Drones and robots are the futuristic and emerging technologies that are defying the need of orthodox delivery systems. These automated vehicles enhance capacity of traditional fleets and are equipped with sensors and simulators that perform specific tasks in an efficient manner. Nowadays, they are in high demand by corporates, food business partners, and various organizations. Usage of drones and robots also proved efficient during COVID-19 pandemic in many ways, such that it minimized human-to-human interactions and eventually mitigated the risk of infections; it also increased capacity enhancement of delivery services when everything was out of order during pandemic. In addition to this, these automated vehicles delivered medical supply and goods even in highly congested areas. The development of this study is the outcome of serious need for service innovation. As explained here, each autonomous technology has been applied in various services ranging from food delivery to medical goods. The main idea is that these autonomous technologies have the potential to minimize congestion and traffic by efficiently and reliably utilizing delivery journeys off the road network. That’s why robotics and automation have become a vast research area in the era of technology. In this chapter, three types of delivery systems are discussed and analyzed using the robot and drone technologies. The system includes AGV systems, AAV systems, and hybrid system that utilizes both robots and drones. AGV includes usage of a fleet of robots that run on pedestrians and sideways without interfering with ongoing road traffic network. AAV consists of fleet of drones operating and delivering food parcels at higher speeds. The third system is the hybrid system which is an amalgamation of ground vehicle system and aerial vehicle systems. In this system, robots are utilized for pick-up services in highly congested areas where drone landing is not feasible, and then package is placed in the drone, which is ready to be delivered from pick-up location to destination. A deep analysis of integration of autonomous and emerging technologies in the context of delivery systems is conducted by taking a place named Connaught Place (CP) into consideration. As it is a highly congested place, it will produce good simulation results when parameters and coordinates are taken during peak hours. The aim of this study is multifold: (a) research into robots and drone delivery systems. It includes scheduling algorithms to calculate average waiting time during short intervals of time. To maintain uniformity, suppose it is calculated at an interval of 10 seconds and average turnaround response time is estimated for drones, robots, and hybrid systems; (b) proposed models of three delivery systems with regard to live coordinates recorded during peak hours at CP in Delhi; (c) analyzing the performance of three systems in context of scheduling, time intervals, and waiting times by using appropriate scheduling strategies; and (d) the chapter ends with conclusion and future scope followed by references. In this chapter, it is discussed that all three proposed systems carry single parcel or order at a time but as a future scope, it can be stated that drones carry multiple parcels at a time to minimize customer waiting time and increases optimization. It would also require safety analysis for such situations. © 2024 selection and editorial matter, Rajendra Kumar, Vishal Jain, Leong Wai Yie and Sunantha Teyarachakul; individual chapters, the contributors.},
	isbn = {978-100096228-4; 978-103240424-0},
	abbrev_source_title = {Convergence of IoT, Blockchain, and Computational Intelligence in Smart Cities},
	type = {Book chapter},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 0}
}

