@ARTICLE{Thantharate2024,
	author = {Thantharate, Pratik and Thantharate, Anurag and Kulkarni, Atul},
	title = {GREENSKY: A fair energy-aware optimization model for UAVs in next-generation wireless networks},
	year = {2024},
	journal = {Green Energy and Intelligent Transportation},
	volume = {3},
