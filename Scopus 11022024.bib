	title = {Performance analysis and multi-objective optimization of a Stirling engine based on MOPSOCD},
	year = {2018},
	journal = {International Journal of Thermal Sciences},
	volume = {124},
	pages = {399 – 406},
	doi = {10.1016/j.ijthermalsci.2017.10.030},
