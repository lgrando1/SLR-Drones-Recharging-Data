@CONFERENCE{Wu2021583,
	author = {Wu, Anping and Lu, Yafei and Guo, Zheng and Hou, Zhongxi},
	title = {Target three-dimensional localization and error analysis for unmanned aerial vehicles},
	year = {2021},
	journal = {Proceedings - 2021 6th International Conference on Automation, Control and Robotics Engineering, CACRE 2021},
	pages = {583 – 592},
	doi = {10.1109/CACRE52464.2021.9501316},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85125449222&doi=10.1109%2fCACRE52464.2021.9501316&partnerID=40&md5=cf5cb2269386eb2b0c9281018fea1512},
	abstract = {A three-dimensional localization method is proposed to calculate the coordinates of a ground-based stationary target for unmanned aerial vehicles (UAVs), which is based on multipoint observations and does not depend on the distance measurement. A recursive least-squares estimation of the relative height from the target to the UAV was carried out using the visual axis projection method, and the estimated value was introduced into the three-dimensional observation model as a measured quantity to meet the observability requirement. A Kalman filtering method was used to estimate the 3D coordinates of the target in real time. To analyze the influence of observation distance and sampling interval on localization, the minimum sample number and average estimation error were used as criteria in Monte Carlo simulations. The influence and sensitivity of UAV position error, attitude angle error, gimbal directional angle error, and image pixel position error were analyzed. The simulation results show that the ground target localization algorithm can effectively calculate the 3D coordinates of the target point in real time, and the localization error is within the allowable range, which has prospects for engineering applications. Reducing the observation distance and sampling interval and using an observing angle of nearly 45° are helpful for improving the precision of target localization. The gimbal directional angle error is the most important factor that influences the accuracy of target localization. © 2021 IEEE.},
